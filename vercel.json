{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app.py"
      }
    ],
    "env": {
      "OPENAI_API_KEY": "<your_openai_api_key>",
      "SECRET_KEY": "<your_secret_key>"
    }
  }
  